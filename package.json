{
  "name": "component-module-generator",
  "displayName": "Component Module Generator",
  "description": "Generate the basic structure of a module react component",
  "version": "0.6.0",
  "engines": {
    "vscode": "^1.35.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/pedroknup/react-component-generator.git"
  },
  "publisher": "pedroknup",
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.generateModuleComponent"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateModuleComponent",
        "title": "Create Module Component"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.14.10",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  }
}
